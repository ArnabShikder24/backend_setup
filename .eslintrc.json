{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "import/prefer-default-export": "off",
    "import/extensions": "off",
    "array-callback-return": "off",
    "no-unused-vars": "off",
    "no-underscore-dangle": "off",
    "camelcase": "off",
    "no-console": "off",
    "max-len": [
      "error",
      {
        "code": 128
      }
    ]
  }
}